  "components": {
    "schemas": {
      "AICPSignInResult": {
        "description": "Class encapsulating the sign in result for an AICP video.",
        "properties": {
          "user": {
            "$ref": "#/components/schemas/AICPUser"
          },
          "access_token": {
            "title": "Access Token",
            "type": "string"
          },
          "refresh_token": {
            "title": "Refresh Token",
            "type": "string"
          }
        },
        "required": [
          "user",
          "access_token",
          "refresh_token"
        ],
        "title": "AICPSignInResult",
        "type": "object"
      },
      "AICPUser": {
        "description": "Class encapsulating the user for an AICP video.",
        "properties": {
          "id": {
            "title": "Id",
            "type": "string"
          },
          "username": {
            "title": "Username",
            "type": "string"
          },
          "email": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Email"
          }
        },
        "required": [
          "id",
          "username"
        ],
        "title": "AICPUser",
        "type": "object"
      },
      "AICPSignInCredentials": {
        "description": "Class encapsulating the sign in credentials for an AICP video.",
        "properties": {
          "username": {
            "title": "Username",
            "type": "string"
          },
          "password": {
            "title": "Password",
            "type": "string"
          }
        },
        "required": [
          "username",
          "password"
        ],
        "title": "AICPSignInCredentials",
        "type": "object"
      },
      "AICPRefreshTokenResult": {
        "description": "Class encapsulating the refresh token result for an AICP video.",
        "properties": {
          "access_token": {
            "title": "Access Token",
            "type": "string"
          },
          "refresh_token": {
            "title": "Refresh Token",
            "type": "string"
          }
        },
        "required": [
          "access_token",
          "refresh_token"
        ],
        "title": "AICPRefreshTokenResult",
        "type": "object"
      },
      "AICPRefreshTokenRequest": {
        "description": "Class encapsulating the refresh token request for an AICP video.",
        "properties": {
          "refresh_token": {
            "title": "Refresh Token",
            "type": "string"
          }
        },
        "required": [
          "refresh_token"
        ],
        "title": "AICPRefreshTokenRequest",
        "type": "object"
      },
      "AICPActor": {
        "description": "Class encapsulating the actor for an AICP video.",
        "properties": {
          "name": {
            "title": "Name",
            "type": "string"
          },
          "bio": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Bio"
          },
          "catch_phrase": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Catch Phrase"
          },
          "physical_description": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Physical Description"
          },
          "speaker": {
            "title": "Speaker",
            "type": "string"
          },
          "speaker_text_temp": {
            "default": 0.7,
            "title": "Speaker Text Temp",
            "type": "number"
          },
          "speaker_waveform_temp": {
            "default": 0.7,
            "title": "Speaker Waveform Temp",
            "type": "number"
          }
        },
        "required": [
          "name",
          "bio",
          "catch_phrase",
          "physical_description",
          "speaker"
        ],
        "title": "AICPActor",
        "type": "object"
      },
      "AICPProjectCreate": {
        "description": "Class encapsulating the project for an AICP video.",
        "properties": {
          "name": {
            "title": "Name",
            "type": "string"
          },
          "description": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Description"
          },
          "actors": {
            "default": [],
            "items": {
              "$ref": "#/components/schemas/AICPActor"
            },
            "title": "Actors",
            "type": "array"
          },
          "seed": {
            "title": "Seed",
            "type": "integer"
          },
          "program_id": {
            "title": "Program Id",
            "type": "string"
          },
          "production_config_id": {
            "title": "Production Config Id",
            "type": "string"
          }
        },
        "required": [
          "name",
          "program_id",
          "production_config_id"
        ],
        "title": "AICPProjectCreate",
        "type": "object"
      }
    },
    "securitySchemes": {
